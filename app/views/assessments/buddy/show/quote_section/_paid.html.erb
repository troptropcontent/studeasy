<%= turbo_frame_tag dom_id(@assessment, :solution), class: "w-full" do %>
    <div class="flex flex-col w-full">
        <%= form_with url: assessment_quote_solution_path(assessment) do |form| %>
            <%= form.button "Assign assessment to me", class: "w-full bg-slate-300 hover:bg-slate-700 text-white font-bold py-2 px-4 rounded-lg focus:outline-none focus:shadow-outline" %>
        <% end %>
    </div>
<% end %>
